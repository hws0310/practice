<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.clone.sns.model.dao.MemberDao">
	<insert id="insertMember" parameterType="memberRegistRequest">
		insert into sns.member(userId,`name`,userName,`password`,createTime)
		values(#{memberRegistRequest.userId},#{memberRegistRequest.name},#{memberRegistRequest.userName},#{memberRegistRequest.password},now());
	</insert>
</mapper>